{
	"listen": 1888,
	
	"requests": {
		"rate": {
			"source": "X-API-Key",
			"span": 300000,
			"accuracy": 10,
			"limit": 1000
		},
		"log": {
			"level": "error"
		}
	},
	
	"instances": {
		"limit": {
			"count": 50,
			"memory": 150000,
			"cpu": 1000
		},
		"storage": "./var/instances"
	},
	
	"log": {
		"console": { "colorize": true, "level": "info" }
	},
	
	"store": {
		"type": "redis",
		"settings": { "host": "127.0.0.1", "port": 6379 }
	}
}